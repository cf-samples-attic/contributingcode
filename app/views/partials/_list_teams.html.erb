<h1> Teams </h1>
<ul id="teams_list">
  <% if @owner %> 
    <% @teams.each do |team| %>
      <li><%= team.name %></li>
  <% end %>

  <% else %>
    <% @teams.each do |team| %>
       <li><%= team.name %>
        <% if team.member_count < 4 %>
          <a href= '<%= join_team_path(team) %>' > Join </a>
        <% end %>
      </li>
    <% end %>
  <% end %>
</ul> 